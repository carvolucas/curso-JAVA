package fundamentos;

public class DesafioAritmeticos {

	public static void main(String[] args) {
		
		int a = (3 + 2) * 6;
		int b = (int) Math.pow(a, 2);
		int c = b / (3 * 2);
		
		double d = (1 - 5) * (2 - 7) / 2;
		int e = (int)Math.pow(d, 2);
		
		var f = (c - e);
		int g = (int)Math.pow(f, 3);
		
		int h = (int)Math.pow(10, 3);
		var i = g / h;
		
		System.out.println(i);
	}
}
