package fundamentos;

public class Inferencia {
	
	public static void main(String[] args) {
		
		double a = 2;
		var b = 13.5;
		System.out.println(b);
				
		var c = "texto";
		System.out.println(c);
			/*
			 * a variavel c foi definida como string (texto) e n pode ser usada como num√©rico no Java.  (vice-versa)	
			 */
		
		double d;
		d = 126.65;
		System.out.println(d);
		}

}
